{
  "pairs.enableCharacters": ["(", "{", "[", "\"", "`", "'"],
  "coc.source.emoji.filetypes": ["markdown"],
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "json",
    "vim",
    "python",
    "C++"
  ],
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "⚠",
  "diagnostic.enableMessage": "jump",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "suggest.completionItemKindLabels": {
    "default": "\uf29c",
    "method": "",
    "function": "",
    "constructor": "",
    "field": "ﰠ",
    "variable": "",
    "class": "ﴯ",
    "interface": "",
    "module": "",
    "property": "ﰠ",
    "unit": "塞",
    "value": "",
    "enum": "",
    "keyword": "",
    "snippet": "",
    "color": "",
    "file": "",
    "reference": "",
    "folder": "",
    "enumMember": "",
    "constant": "",
    "struct": "פּ",
    "event": "",
    "operator": "",
    "typeParameter": ""
  },
  "tsserver.implicitProjectConfig.experimentalDecorators": true,
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "snippets.ultisnips.usePythonx": false,
  "eslint.enable": true,
  "eslint.exeArgv": [],
  "eslint.packageManager": ["npm", "yarn"],
  "eslint.alwaysShowStatus": true,
  "eslint.nodeEnv": null,
  "eslint.autoFixOnSave": true,
  "eslint.quiet": true,
  "eslint.onIgnoredFiles": "warn",
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "json",
    "vim",
    "python",
    "C++"
  ],
  "eslint.autoFixOnSave": true,
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  },
  "snippets.userSnippetsDirectory": "~/AppData/Local/nvim/snips"
}
